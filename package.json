    "@types/node": "^24.3.1",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "semantic-release": "^24.2.7",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2"
